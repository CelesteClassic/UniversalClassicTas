newline=[[

]]

world=split(mapdata,newline,false)
for i=1,#world do
  local str=world[i]
  world[i]={}
  for ii=1,#str/2 do
    local hx=sub(str,2*(ii-1)+1,2*(ii-1)+2)
    if hx=="zz" then hx="101" end
    world[i][ii]=tonum("0x"..hx)
  end
end

cam_x,cam_y=0,(#world-16)*8-8

function mget(x,y)
  local row=world[y+1]
  local col=row and x+2-row[1] or 1
  return col > 1 and row[col] or 37
end

function map(layer)
  local origin_y,origin_x,cell=flr(cam_y/8),flr(cam_x/8)
  local cell
  for y=0,17 do
    for x=0,17 do
	  local row=world[origin_y+y]
	  local col=row and origin_x+x-row[1]+1 or 1
	  local cell=col > 1 and row[col] or 37
	  if fget(cell,layer) then
	    spr(cell,(origin_x+x-1)*8,(origin_y+y-1)*8)
	  end
	end
  end
end

function update_diffs()
  menuitem(1,"mode: "..modes[difficulty][1], function()end)
  menuitem(2,difficulty==#modes and " > --" or " > difficulty++",function()
    difficulty=min(difficulty+1,#modes)
    update_diffs()
    load_room(0,0)
  end)
  menuitem(3,difficulty==1 and " > --" or " > difficulty--",function()
    difficulty=max(difficulty-1,1)
    update_diffs()
    load_room(0,0)
  end)
end
update_diffs()

menuitem(4,"og/new graphics", function()
	og=not og
end)

-- [entry point]

function begin_game()
  max_djump,deaths,frames,seconds,minutes,music_timer=1,0,0,0,0,0
  music(0,0,7)
  load_room(0,0)
end

-- [player entity]

player={
  init=function(q) 
    q.grace,q.jbuffer=0,0
    q.djump=max_djump
    q.dsh_time,q.dsh_effect_time=0,0
    q.dsh_target_x,q.dsh_target_y=0,0
    q.dsh_accel_x,q.dsh_accel_y=0,0
    q.hitbox=rectangle(1,3,6,5)
    q.spr_off=0
    q.solids=true
	q.dead=false
    create_hair(q)
	plyr=q
	spawned=true
  end,
  update=function(q)
    if pause_player then
      return
    end
    
    -- horizontal input
    local h_input=btn(‚û°Ô∏è) and 1 or btn(‚¨ÖÔ∏è) and -1 or 0
	
	-- victory check
	if not score and q.y<15*8 then
		score=lavapos
	end
	
	-- lava death check
	if q.y>lavapos then
	  kill_player(q)
	end
    
    -- spike collision / bottom death
    if spikes_at(q.x+q.hitbox.x,q.y+q.hitbox.y,q.hitbox.w,q.hitbox.h,q.spd.x,q.spd.y) --[[or 
      q.y>128]] then
      kill_player(q)
    end

    -- on ground checks
    local on_ground=q.is_solid(0,1)
    
    -- landing smoke
    if on_ground and not q.was_on_ground then
      init_smoke(q.x,q.y+4)
    end

    -- jump and dash input
    local jump,dash=btn(üÖæÔ∏è) and not q.p_jump,btn(‚ùé) and not q.p_dash
    q.p_jump,q.p_dash=btn(üÖæÔ∏è),btn(‚ùé)

    -- jump buffer
    if jump then
      q.jbuffer=4
    elseif q.jbuffer>0 then
      q.jbuffer-=1
    end
    
    -- grace frames and dash restoration
    if on_ground then
      q.grace=6
      if q.djump<max_djump then
        psfx(54)
        q.djump=max_djump
      end
    elseif q.grace>0 then
      q.grace-=1
    end

    -- dash effect timer (for dash-triggered events, e.g., berry blocks)
    q.dsh_effect_time-=1

    -- dash startup period, accel toward dash target speed
    if q.dsh_time>0 then
      init_smoke(q.x,q.y)
      q.dsh_time-=1
      q.spd=vector(
        appr(q.spd.x,q.dsh_target_x,q.dsh_accel_x),
        appr(q.spd.y,q.dsh_target_y,q.dsh_accel_y)
      )
    else
      -- x movement
      local maxrun=1
      local accel=q.is_ice(0,1) and 0.05 or on_ground and 0.6 or 0.4
      local deccel=0.15
    
      -- set x speed
      q.spd.x=abs(q.spd.x)<=1 and 
        appr(q.spd.x,h_input*maxrun,accel) or 
        appr(q.spd.x,sign(q.spd.x)*maxrun,deccel)
      
      -- facing direction
      if q.spd.x~=0 then
        q.flip.x=(q.spd.x<0)
      end

      -- y movement
      local maxfall=2
    
      -- wall slide
      if h_input~=0 and q.is_solid(h_input,0) and not q.is_ice(h_input,0) then
        maxfall=0.4
        -- wall slide smoke
        if rnd(10)<2 then
          init_smoke(q.x+h_input*6,q.y)
        end
      end

      -- apply gravity
      if not on_ground then
        q.spd.y=appr(q.spd.y,maxfall,abs(q.spd.y)>0.15 and 0.21 or 0.105)
      end

      -- jump
      if q.jbuffer>0 then
        if q.grace>0 then
          -- normal jump
          psfx(1)
          q.jbuffer=0
          q.grace=0
          q.spd.y=-2
          init_smoke(q.x,q.y+4)
        else
          -- wall jump
          local wall_dir=(q.is_solid(-3,0) and -1 or q.is_solid(3,0) and 1 or 0)
          if wall_dir~=0 then
            psfx(2)
            q.jbuffer=0
            q.spd=vector(-wall_dir*(maxrun+1),-2)
            if not q.is_ice(wall_dir*3,0) then
              -- wall jump smoke
              init_smoke(q.x+wall_dir*6,q.y)
            end
          end
        end
      end
    
      -- dash
      local d_full=5
      local d_half=3.5355339059 -- 5 * sqrt(2)
    
      if q.djump>0 and dash then
        init_smoke(q.x,q.y)
        q.djump-=1   
        q.dsh_time=4
        dashes=dashes+1
        q.dsh_effect_time=10
        -- vertical input
        local v_input=btn(‚¨ÜÔ∏è) and -1 or btn(‚¨áÔ∏è) and 1 or 0
        -- calculate dash speeds
        q.spd=vector(
          h_input~=0 and h_input*(v_input~=0 and d_half or d_full) or (v_input~=0 and 0 or q.flip.x and -1 or 1),
          v_input~=0 and v_input*(h_input~=0 and d_half or d_full) or 0
        )
        -- effects
        psfx(3)
        freeze=2
        shake=6
        -- dash target speeds and accels
        q.dsh_target_x=2*sign(q.spd.x)
        q.dsh_target_y=(q.spd.y>=0 and 2 or 1.5)*sign(q.spd.y)
        q.dsh_accel_x=q.spd.y==0 and 1.5 or 1.06066017177 -- 1.5 * sqrt()
        q.dsh_accel_y=q.spd.x==0 and 1.5 or 1.06066017177
      elseif q.djump<=0 and dash then
        -- failed dash smoke
        psfx(9)
        init_smoke(q.x,q.y)
      end
    end
    
    -- animation
    q.spr_off+=0.25
    q.spr = not on_ground and (q.is_solid(h_input,0) and 5 or 3) or  -- wall slide or mid air
      btn(‚¨áÔ∏è) and 6 or -- crouch
      btn(‚¨ÜÔ∏è) and 7 or -- look up
      1+(q.spd.x~=0 and h_input~=0 and q.spr_off%4 or 0) -- walk or stand
    
    -- was on the ground
    q.was_on_ground=on_ground
	if not q.dead then
	  cam_x,cam_y=clamp(q.x-64,0,(#world[1]+world[1][1]-1)*8-128),clamp(q.y-64,0,#world*8-128-8)
	end
  end,
  
  draw=function(q)
    if q.x<-1 or q.x>(#world[1]+world[1][1]-1)*8-7 then 
      q.x=clamp(q.x,-1,(#world[1]+world[1][1]-1)*8-7)
      q.spd.x=0
    end
    -- draw player hair and sprite
    set_hair_color(q.djump)
    draw_hair(q,q.flip.x and -1 or 1)
    spr(q.spr,q.x,q.y,1,1,q.flip.x,q.flip.y)   
    unset_hair_color()
  end
}

function create_hair(obj)
  obj.hair={}
  for i=1,5 do
    add(obj.hair,vector(obj.x,obj.y))
  end
end

function set_hair_color(djump)
  pal(8,djump==1 and 8 or djump==2 and 7+(frames\3)%2*4 or 12)
end

function draw_hair(obj,facing)
  local last=vector(obj.x+4-facing*2,obj.y+(btn(‚¨áÔ∏è) and 4 or 3))
  for i,h in pairs(obj.hair) do
    h.x+=(last.x-h.x)/1.5
    h.y+=(last.y+0.5-h.y)/1.5
    circfill(h.x,h.y,clamp(4-i,1,2),8)
    last=h
  end
end

function unset_hair_color()
  pal(8,8)
end

-- [other entities]

player_spawn={
  init=function(q)
    sfx(4)
    q.spr=3
    q.target=q.y
    q.y=#world*8
    q.spd.y=-4
    q.state=0
    q.delay=0
    create_hair(q)
	cam_x,cam_y=0,(#world-16)*8-8
  end,
  update=function(q)
    -- jumping up
    if q.state==0 then
      if q.y<q.target+16 then
        q.state=1
        q.delay=3
      end
    -- falling
    elseif q.state==1 then
      q.spd.y+=0.5
      if q.spd.y>0 then
        if q.delay>0 then
          -- stall at peak
          q.spd.y=0
          q.delay-=1
        elseif q.y>q.target then
          -- clamp at target y
          q.y=q.target
          q.spd=vector(0,0)
          q.state=2
          q.delay=5
          shake=5
          init_smoke(q.x,q.y+4)
          sfx(5)
        end
      end
    -- landing and spawning player object
    elseif q.state==2 then
      q.delay-=1
      q.spr=6
      if q.delay<0 then
        destroy_object(q)
        init_object(player,q.x,q.y)
      end
    end
  end,
  draw=function(q)
    set_hair_color(max_djump)
    draw_hair(q,1)
    spr(q.spr,q.x,q.y)
    unset_hair_color()
  end
}

spring={
  init=function(q)
    q.hide_in=0
    q.hide_for=0
  end,
  update=function(q)
    if q.hide_for>0 then
      q.hide_for-=1
      if q.hide_for<=0 then
        q.spr=18
        q.delay=0
      end
    elseif q.spr==18 then
      local hit=q.check(player,0,0)
      if hit and hit.spd.y>=0 then
        q.spr=19
        hit.y=q.y-4
        hit.spd.x*=0.2
        hit.spd.y=-3
        hit.djump=max_djump
        q.delay=10
        init_smoke(q.x,q.y)
        -- crumble below spring
        local below=q.check(fall_floor,0,1)
        if below then
          break_fall_floor(below)
        end
        psfx(8)
      end
    elseif q.delay>0 then
      q.delay-=1
      if q.delay<=0 then 
        q.spr=18 
      end
    end
    -- begin hiding
    if q.hide_in>0 then
      q.hide_in-=1
      if q.hide_in<=0 then
        q.hide_for=60
        q.spr=0
      end
    end
  end
}

function break_spring(obj)
  obj.hide_in=15
end

balloon={
  init=function(q) 
    q.offset=rnd(1)
    q.start=q.y
    q.timer=0
    q.hitbox=rectangle(-1,-1,10,10)
  end,
  update=function(q) 
    if q.spr==22 then
      q.offset+=0.01
      q.y=q.start+sin(q.offset)*2
      local hit=q.check(player,0,0)
      if hit and hit.djump<max_djump then
        psfx(6)
        init_smoke(q.x,q.y)
        hit.djump=max_djump
        q.spr=0
        q.timer=60
      end
    elseif q.timer>0 then
      q.timer-=1
    else 
     psfx(7)
     init_smoke(q.x,q.y)
      q.spr=22 
    end
  end,
  draw=function(q)
    if q.spr==22 then
      spr(13+(q.offset*8)%3,q.x,q.y+6)
      spr(q.spr,q.x,q.y)
    end
  end
}

fall_floor={
  init=function(q)
    q.state=0
  end,
  update=function(q)
    -- idling
    if q.state==0 then
      if q.check(player,0,-1) or q.check(player,-1,0) or q.check(player,1,0) then
        break_fall_floor(q)
      end
    -- shaking
    elseif q.state==1 then
      q.delay-=1
      if q.delay<=0 then
        q.state=2
        q.delay=60--how long it hides for
        q.collideable=false
      end
    -- invisible, waiting to reset
    elseif q.state==2 then
      q.delay-=1
      if q.delay<=0 and not q.check(player,0,0) then
        psfx(7)
        q.state=0
        q.collideable=true
        init_smoke(q.x,q.y)
      end
    end
  end,
  draw=function(q)
    if q.state~=2 then
      if q.state~=1 then
        spr(23,q.x,q.y)
      else
        spr(23+(15-q.delay)/5,q.x,q.y)
      end
    end
  end
}

function break_fall_floor(obj)
 if obj.state==0 then
  psfx(15)
    obj.state=1
    obj.delay=15--how long until it falls
    init_smoke(obj.x,obj.y)
    local hit=obj.check(spring,0,-1)
    if hit then
      break_spring(hit)
    end
  end
end

smoke={
  init=function(q)
    q.spd=vector(0.3+rnd(0.2),-0.1)
    q.x+=-1+rnd(2)
    q.y+=-1+rnd(2)
    q.flip=vector(maybe(),maybe())
  end,
  update=function(q)
    q.spr+=0.2
    if q.spr>=32 then
      destroy_object(q)
    end
  end
}

function init_smoke(x,y)
  init_object(smoke,x,y,29)
end

fruit={
  if_not_fruit=true,
  init=function(q) 
    q.start=q.y
    q.off=0
  end,
  update=function(q)
   local hit=q.check(player,0,0)
    if hit then
      hit.djump=max_djump
      sfx_timer=20
      sfx(13)
      fruits=fruits+1
      init_object(lifeup,q.x,q.y)
      destroy_object(q)
    end
    q.off+=1
    q.y=q.start+sin(q.off/40)*2.5
  end
}

fly_fruit={
  if_not_fruit=true,
  init=function(q) 
    q.start=q.y
    q.fly=false
    q.step=0.5
    q.sfx_delay=8
	q.last_dashes=0
  end,
  update=function(q)
    --fly away
    if q.fly then
     if q.sfx_delay>0 then
      q.sfx_delay-=1
      if q.sfx_delay<=0 then
       sfx_timer=20
       sfx(14)
      end
     end
      q.spd.y=appr(q.spd.y,-3.5,0.25)
      if q.y<-16 then
        destroy_object(q)
      end
    -- wait
    else
      if plyr and dashes>q.last_dashes and ((plyr.x<q.x+56 and plyr.x>q.x-56) and (plyr.y<q.y+56 and plyr.y>q.y-56)) then
        q.fly=true
      end
      q.step+=0.05
      q.spd.y=sin(q.step)*0.5
    end
    -- collect
    local hit=q.check(player,0,0)
    if hit then
      hit.djump=max_djump
      sfx_timer=20
      sfx(13)
      fruits=fruits+1
      init_object(lifeup,q.x,q.y)
      destroy_object(q)
    end
	q.last_dashes=dashes
  end,
  draw=function(q)
    local off=0
    if not q.fly then
      if sin(q.step)<0 then
        off=1+max(0,sign(q.y-q.start))
      end
    else
      off=(off+0.25)%3
    end
    spr(45+off,q.x-6,q.y-2,1,1,true)
    spr(q.spr,q.x,q.y)
    spr(45+off,q.x+6,q.y-2)
  end
}

lifeup={
  init=function(q)
    q.spd.y=-0.25
    q.duration=30
    q.x-=2
    q.y-=4
    q.flash=0
  end,
  update=function(q)
    q.duration-=1
    if q.duration<=0 then
      destroy_object(q)
    end
  end,
  draw=function(q)
    q.flash+=0.5
    ?"1000",q.x-2,q.y,7+q.flash%2
  end
}

fake_wall={
  if_not_fruit=true,
  update=function(q)
    q.hitbox.w,q.hitbox.h=18,18
    local hit=q.check(player,-1,-1)
    if hit and hit.dsh_effect_time>0 then
      hit.spd=vector(-sign(hit.spd.x)*1.5,-1.5)
      hit.dsh_time=-1
      sfx_timer=20
      sfx(16)
      destroy_object(q)
      init_smoke(q.x,q.y)
      init_smoke(q.x+8,q.y)
      init_smoke(q.x,q.y+8)
      init_smoke(q.x+8,q.y+8)
      init_object(fruit,q.x+4,q.y+4,26)
    end
    q.hitbox.w,q.hitbox.h=16,16
  end,
  draw=function(q)
    sspr(0,32,16,16,q.x,q.y)
  end
}

key={
  if_not_fruit=true,
  init=function(q)
	q.keynum=#keys+1
	keys[#keys+1]=false
  end,
  update=function(q)
    local was=flr(q.spr)
    q.spr=9.5+sin(frames/30)
    if q.spr==10 and q.spr~=was then
      q.flip.x=not q.flip.x
    end
    if q.check(player,0,0) then
      sfx(23)
      sfx_timer=10
      destroy_object(q)
      keys[q.keynum]=true
    end
  end
}

chest={
  if_not_fruit=true,
  init=function(q)
    q.x-=4
    q.start=q.x
    q.timer=20
	chestsspawned=chestsspawned+1
	q.chestnum=chestsspawned
  end,
  update=function(q)
    if keys[q.chestnum]==true then
      q.timer-=1
      q.x=q.start-1+rnd(3)
      if q.timer<=0 then
        sfx_timer=20
        sfx(16)
        init_object(fruit,q.x,q.y-4,26)
        destroy_object(q)
      end
    end
  end
}

platform={
  init=function(q)
    q.xl=(q.y>8*(#world-159) and 8*(55-16)) or (q.y>8*(#world-178) and 8*(78-16)) or (q.y>8*(#world-282) and 8*(150-16)) or 0
	q.seen=true
	if q.y<8*(#world-159) and q.y>8*(#world-178) then
	  q.seen=false
	end
	q.xh=q.xl+144
    q.x-=4
    q.hitbox.w=16
    q.last=q.x
    q.dir=q.spr==11 and -1 or 1
  end,
  update=function(q)
    if q.seen then
      q.spd.x=q.dir*0.65
      if q.x<q.xl then q.x=q.xh
      elseif q.x>q.xh then q.x=q.xl end
      if not q.check(player,0,0) then
        local hit=q.check(player,0,-1)
        if hit then
          hit.move_x(q.x-q.last,1)
        end
      end
      q.last=q.x
	elseif plyr and ((plyr.x<q.x+48 and plyr.x>q.x-48) and (plyr.y<q.y+48 and plyr.y>q.y-48)) then
	  q.seen=true
	end
  end,
  draw=function(q)
    spr(11,q.x,q.y-1)
    spr(12,q.x+8,q.y-1)
  end
}

message={
  draw=function(q)
    if difficulty>=2 then
      q.text="-- celeste mountain --#this memorial is#dedicated to the penguin#perishing on the climb"
	else
	  q.text="-- celeste mountain --#this memorial is#dedicated to those who#perished on the climb"
	end
    if q.check(player,4,0) then
