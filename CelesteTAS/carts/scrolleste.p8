-- [tile dict]
tiles={
  [1]=player_spawn,
  [8]=key,
  [11]=platform,
  [12]=platform,
  [18]=spring,
  [20]=chest,
  [22]=balloon,
  [23]=fall_floor,
  [26]=fruit,
  [28]=fly_fruit,
  [64]=fake_wall,
  [86]=message,
  [96]=big_chest,
  [118]=flag
}

-- [object functions]

function init_object(type,x,y,tile)
  local obj={
    type=type,
    collideable=true,
    solids=false,
    spr=tile,
    flip=vector(false,false),
    x=x,
    y=y,
    hitbox=rectangle(0,0,8,8),
    spd=vector(0,0),
    rem=vector(0,0),
  }

  function obj.is_solid(ox,oy)
    return (oy>0 and not obj.check(platform,ox,0) and obj.check(platform,ox,oy)) or
           tile_flag_at(obj.x+obj.hitbox.x+ox,obj.y+obj.hitbox.y+oy,obj.hitbox.w,obj.hitbox.h,0) or 
           obj.check(fall_floor,ox,oy) or
           obj.check(fake_wall,ox,oy)
  end
  
  function obj.is_ice(ox,oy)
    return tile_flag_at(obj.x+obj.hitbox.x+ox,obj.y+obj.hitbox.y+oy,obj.hitbox.w,obj.hitbox.h,4)
  end
  
  function obj.check(type,ox,oy)
    if type==player then
	  if plyr and
        plyr.x+plyr.hitbox.x+plyr.hitbox.w>obj.x+obj.hitbox.x+ox and 
        plyr.y+plyr.hitbox.y+plyr.hitbox.h>obj.y+obj.hitbox.y+oy and
        plyr.x+plyr.hitbox.x<obj.x+obj.hitbox.x+obj.hitbox.w+ox and 
        plyr.y+plyr.hitbox.y<obj.y+obj.hitbox.y+obj.hitbox.h+oy then
        return plyr
	  else
	    return false
	  end
	else
	  for other in all(objects) do
		if other and other.type==type and other~=obj and other.collideable and
		  other.x+other.hitbox.x+other.hitbox.w>obj.x+obj.hitbox.x+ox and 
		  other.y+other.hitbox.y+other.hitbox.h>obj.y+obj.hitbox.y+oy and
		  other.x+other.hitbox.x<obj.x+obj.hitbox.x+obj.hitbox.w+ox and 
          other.y+other.hitbox.y<obj.y+obj.hitbox.y+obj.hitbox.h+oy then
          return other
        end
      end
    end
  end
  
  function obj.move(ox,oy)
    -- x movement
    obj.rem.x+=ox
    local amount=flr(obj.rem.x+0.5)
    obj.rem.x-=amount
    obj.move_x(amount,0)
    -- y movement
    obj.rem.y+=oy
    amount=flr(obj.rem.y+0.5)
    obj.rem.y-=amount
    obj.move_y(amount)
  end
  
  function obj.move_x(amount,start)
    if obj.solids then
      local step=sign(amount)
      for i=start,abs(amount) do
        if not obj.is_solid(step,0) then
          obj.x+=step
        else
          obj.spd.x=0
          obj.rem.x=0
          break
        end
      end
    else
      obj.x+=amount
    end
  end
  
  function obj.move_y(amount)
    if obj.solids then
      local step=sign(amount)
      for i=0,abs(amount) do
      if not obj.is_solid(0,step) then
          obj.y+=step
        else
          obj.spd.y=0
          obj.rem.y=0
          break
        end
      end
    else
      obj.y+=amount
    end
  end

  add(objects,obj)

  if obj.type.init then
    obj.type.init(obj)
  end

  return obj
end

function destroy_object(obj)
  del(objects,obj)
end

function kill_player(obj)
  sfx_timer=12
  sfx(0)
  deaths+=1
  shake=10
  destroy_object(obj)
  obj.dead=true
  max_djump=1
  plyr=nil
  spawned=false
  dead_particles={}
  for dir=0,0.875,0.125 do
    add(dead_particles,{
      x=obj.x+4,
      y=obj.y+4,
      t=2,
      dx=sin(dir)*3,
      dy=cos(dir)*3
    })
  end
  restart_room()
end

-- [room functions]

function restart_room()
  delay_restart=15
end


function load_room(x,y)
  dashes=0
  fruits=0
  score=nil
  keys={}
  chestsspawned=0
  lavapos=#world*8
  max_djump=1
  new_bg=nil
  plyr=nil
  spawned=false
  if postgem then
    music(0,0,7)
    postgem=false
  end
  --remove existing objects
  foreach(objects,destroy_object)
  -- entities
  for ty=1,#world do
    for tx=1,#world[ty]+world[ty][1]-1 do
      local tile=mget(tx-1,ty-1)
      if tiles[tile] then
        init_object(tiles[tile],(tx-1)*8,(ty-1)*8,tile)
      end
    end
  end
end

-- [main update loop]

function _update()
  if spawned then
      if lavapos>cam_y+128 then
        lavapos-=modes[difficulty][4]
	  elseif lavapos>plyr.y+20 then
	    lavapos-=modes[difficulty][3]
	  else
	    lavapos-=modes[difficulty][2]
	  end
	  if lavapos<15*8 then
	    lavapos=15*8 
	  elseif score and (score>=200) and difficulty>=2 then
	    lavapos=score
	  end
  end
  frames=(frames+1)%30
  if frames==0 then
    seconds=(seconds+1)%60
    if seconds==0 then
      minutes+=1
    end
  end
  
  if music_timer>0 then
    music_timer-=1
    if music_timer<=0 then
      music(20,0,7)
    end
  end
  
  if sfx_timer>0 then
    sfx_timer-=1
  end
  
  -- cancel if freeze
  if freeze>0 then 
    freeze-=1
    return
  end
  
  -- restart (soon)
  if delay_restart>0 then
    delay_restart-=1
    if delay_restart==0 then
      load_room(0,0)
    end
  end

  -- update each object
  foreach(objects,function(obj)
    obj.move(obj.spd.x,obj.spd.y)
    if obj.type.update then
      obj.type.update(obj)
    end
  end)
end

  -- start game
  begin_game()

-- [drawing functions]

function _draw()
  if freeze>0 then
    return
  end
  
  -- camera?
  camera(cam_x,cam_y)
  -- reset all palette values
  pal()
  
  pal(12,og and 12 or 5)
  pal(13,og and 7 or 6)
  -- draw bg color (pad for screenshake)
  cls()
  rectfill(cam_x,cam_y,cam_x+127,cam_y+127,flash_bg and frames/5 or new_bg and 2 or 0)

  -- draw bg terrain
  --map(2)

  -- draw clouds + orb chest
  foreach(objects, function(o)
    if o.type==platform then
      draw_object(o)
    end
  end)

  -- draw lava lavayer 1
  for dx=-16,132,4 do
    local dh=sin(time()+dx/152+0.05)
    rectfill(cam_x+dx,lavapos+dh,cam_x+dx+4,max(cam_y+136,lavapos),8)
    rectfill(cam_x+dx,lavapos+dh,cam_x+dx+4,lavapos+dh+1,new_bg and 14 or 2)
  end
  
  -- draw terrain (offset if title screen)
  map(1)
  
  -- draw objects
  foreach(objects, function(o)
    if o.type~=platform then
      draw_object(o)
    end
  end)
  
  foreach(dead_particles,function(p)
    p.x+=p.dx
    p.y+=p.dy
    p.t-=0.2
    if p.t<=0 then
      del(dead_particles,p)
    end
    rectfill(p.x-p.t,p.y-p.t,p.x+p.t,p.y+p.t,14+p.t*5%2)
  end)
  
  -- draw lava layer 2
  for dx=-16,132,4 do
    local dh=sin(time()+dx/152)
    rectfill(cam_x+dx,lavapos+4+dh,cam_x+dx+4,max(cam_y+136,lavapos)+4,8)
  end
	
  -- draw fg terrain (not a thing)
  --map(room.x*16,room.y*16,0,0,16,16,8)
end

function draw_object(obj)
  if obj.type.draw then
    obj.type.draw(obj)
  elseif obj.spr then
    spr(obj.spr,obj.x,obj.y,1,1,obj.flip.x,obj.flip.y)
  end
end

function draw_time(x,y)
  rectfill(x,y,x+32,y+6,0)
  ?two_digit_str(minutes\60)..":"..two_digit_str(minutes%60)..":"..two_digit_str(seconds),x+1,y+1,7
end

function two_digit_str(x)
  return x<10 and "0"..x or x
end

function crectfill(x1,y1,x2,y2,c)
  if x1<128 and x2>0 and y1<128 and y2>0 then
    rectfill(max(0,x1),max(0,y1),min(127,x2),min(127,y2),c)
  end
end

-- [helper functions]

function clamp(val,a,b)
  return max(a,min(b,val))
end

function appr(val,target,amount)
  return val>target and max(val-amount,target) or min(val+amount,target)
end

function sign(v)
  return v~=0 and sgn(v) or 0
end

function maybe()
  return rnd(1)<0.5
end

function tile_flag_at(x,y,w,h,flag)
  for i=max(0,x\8),min(#world[1]+world[1][1]-1,(x+w-1)/8) do
    for j=max(0,y\8),min(#world,(y+h-1)/8) do
      if fget(tile_at(i,j),flag) then
        return true
      end
    end
  end
end

function tile_at(x,y)
  return mget(x,y)
end

function spikes_at(x,y,w,h,xspd,yspd)
  for i=max(0,x\8),min(#world[1]+world[1][1]-1,(x+w-1)/8) do
    for j=max(0,y\8),min(#world,(y+h-1)/8) do
      local tile=tile_at(i,j)
      if (tile==17 and ((y+h-1)%8>=6 or y+h==j*8+8) and yspd>=0) or
         (tile==27 and y%8<=2 and yspd<=0) or
         (tile==43 and x%8<=2 and xspd<=0) or
         (tile==59 and ((x+w-1)%8>=6 or x+w==i*8+8) and xspd>=0) then
         return true
      end
    end
  end
end

